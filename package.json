    "gatsby-image": "^2.7.0",
    "gatsby-plugin-advanced-sitemap": "^1.5.6",
    "gatsby-plugin-canonical-urls": "^2.6.0",
    "gatsby-plugin-create-client-paths": "^2.6.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-mdx": "^2.0.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "postcss": "^8.2.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "prettier": "2.1.2"
